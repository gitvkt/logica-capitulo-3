//Escrever um algoritmo que leia dois valores inteiro distintos e informe qual é o maior.


algoritmo "exercídio 1 - pratica 9"
var

   NUMERO1:inteiro
   NUMERO2:inteiro

inicio

   escreva ("Digite um número inteiro")
   leia(NUMERO1)
   escreva ("Digite outro número inteiro ")
   leia(NUMERO2)

   se NUMERO1>NUMERO2 então
      escreva (NUMERO1)
   senão
      escreva (NUMERO2)
   fimse

fimalgoritmo
